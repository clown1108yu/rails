<div class="notes">
  <span class="mark">*</span> 印のついた項目は入力必須です。
</div>
<div>
  <%= f.label :email, 'メールアドレス', class: 'required' %>
  <%= f.email_field :email, size: 32, required: true %>
</div>
<div>
  <%= f.label :family_name, '氏名', class: 'required' %>
  <%= f.text_field :family_name, requried: true %>
  <%= f.text_field :given_name, requried: true %>
</div>
<div>
  <%= f.label :family_name_kana, 'フリガナ', class: 'required' %>
  <%= f.text_field :family_name_kana, requried: true %>
  <%= f.text_field :given_name_kana, requried: true %>
</div>
